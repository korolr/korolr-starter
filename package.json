{
  "name": "korolr-starter",
  "version": "1.0.0",
  "description": "Gulp 4 and webpack",
  "main": "index.js",
  "scripts": {
    "lint": "eslint --debug app/js/",
    "lint:write": "eslint --debug app/js/ --fix",
    "prettier": "prettier --write app/js/**/*.js"
  },
  "author": "korolr",
  "license": "ISC",
  "devDependencies": {
    "@babel/core": "^7.0.0",
    "@babel/preset-env": "^7.0.0",
    "babel-eslint": "^10.0.1",
    "babel-loader": "^8.0.4",
    "browser-sync": "^2.24.4",
    "eslint": "^5.9.0",
    "eslint-config-airbnb": "^17.0.0",
    "eslint-config-jest-enzyme": "^7.0.1",
    "eslint-config-prettier": "^3.3.0",
    "eslint-plugin-babel": "^5.1.0",
    "eslint-plugin-import": "^2.12.0",
    "eslint-plugin-jest": "^22.1.2",
    "eslint-plugin-jsx-a11y": "^6.0.3",
    "eslint-plugin-prettier": "^3.0.0",
    "eslint-plugin-react": "^7.9.1",
    "gulp": "^4.0.0",
    "gulp-concat": "^2.6.1",
    "gulp-html-beautify": "^1.0.1",
    "gulp-imagemin": "^5.0.3",
    "gulp-nunjucks-render": "^2.2.2",
    "gulp-plumber": "^1.2.0",
    "gulp-rename": "^1.2.2",
    "gulp-util": "^3.0.8",
    "husky": "^1.2.0",
    "lint-staged": "^8.1.0",
    "node-sass": "^4.11.0",
    "prettier": "^1.15.3",
    "sass-loader": "^7.1.0",
    "stylelint-config-prettier": "^4.0.0",
    "stylelint-config-recommended": "^2.1.0",
    "stylelint-config-standard": "^18.2.0",
    "stylelint-order": "^2.0.0",
    "vinyl-buffer": "^1.0.1",
    "vinyl-source-stream": "^2.0.0",
    "webpack": "^4.27.1",
    "webpack-bundle-analyzer": "^3.0.3",
    "webpack-cli": "^3.1.2",
    "webpack-dev-server": "^3.1.10",
    "extract-text-webpack-plugin": "^3.0.2",
    "style-loader": "^0.23.1",
    "css-loader": "^2.0.0",
    "postcss-loader": "^3.0.0",
    "autoprefixer": "^9.4.2"
  },
  "dependencies": {
    "loadash": "^1.0.0",
    "ramda": "^0.26.1",
    "velocity-animate": "^1.5.2"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "app/js/**/*.js": [
      "npm run lint:write",
      "prettier --write",
      "git add"
    ],
    "*.scss": [
      "stylelint --fix --config ./.stylelintrc",
      "git add"
    ]
  }
}
